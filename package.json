{
  "name": "jungfrau",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run jungfrau:serve-ssr",
    "serve-beta:ssr": "node dist/beta/server/main.js",
    "serve-prod:ssr": "node dist/prod/server/main.js",
    "build-beta:ssr": "ng build --configuration beta && ng run jungfrau:server:beta",
    "build-prod:ssr": "ng build --configuration production && ng run jungfrau:server:production",
    "prerender": "ng run jungfrau:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/elements": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/platform-server": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/ssr": "^17.0.8",
    "bootstrap-icons": "^1.11.2",
    "domino": "^2.1.6",
    "express": "^4.18.2",
    "html-loader": "^4.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular/cli": "~17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.2"
  },
  "description": "",
